{
  "name": "front",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start": "vite",
    "serve": "vite preview",
    "generate-client": "openapi --useUnionTypes --input ./openapi.json --output ./src/5esheets-client",
    "generate-effect-parser": "peggy --plugin ./node_modules/ts-pegjs/dist/tspegjs -o src/effects/parser/index.ts src/effects/parser/grammar.pegjs",
    "test": "vitest --run",
    "lint": "eslint src",
    "prettier": "prettier -w .",
    "prettier-check": "prettier -c .",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "devDependencies": {
    "@solid-primitives/i18n": "^2.0.0",
    "@solid-primitives/resource": "^0.1.0",
    "@solidjs/meta": "^0.28.6",
    "@solidjs/router": "^0.8.2",
    "@solidjs/testing-library": "^0.8.4",
    "@testing-library/jest-dom": "^6.1.4",
    "@types/debug": "^4.1.10",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-solid": "^0.13.0",
    "jsdom": "^22.1.0",
    "openapi-typescript-codegen": "^0.25.0",
    "peggy": "^3.0.2",
    "prettier": "^3.0.3",
    "solid-js": "^1.8.4",
    "solid-styled": "^0.9.2",
    "ts-pegjs": "^4.2.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-solid": "^2.7.2",
    "vite-plugin-solid-styled": "^0.8.3",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@types/dompurify": "^3.0.4",
    "dompurify": "^3.0.6",
    "marked": "^9.1.3"
  }
}
